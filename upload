
az keyvault key create -n mykey -p software --vault-name minjarvalt
az keyvault set-policy -n minjarvalt --object-id b11f37d0-bf72-411d-8a2d-f8e51a70176f -g Geeks_RG --key-permissions wrapKey unwrapKey get list 

az sql server key create -g "Geeks_RG" -s "tdegenpacttest" -k https://minjarvalt.vault.azure.net/keys/mykey/eeb07a9555b34b10a699f3903641a54a

az resource update --id $(az keyvault show --name minjarvaults -o tsv | awk '{print $1}') --set properties.enableSoftDelete=true
az sql server tde-key set -g Geeks_RG -s tdegenpacttest -t AzureKeyVault -k https://minjarvalt.vault.azure.net/keys/mykey/eeb07a9555b34b10a699f3903641a54a


  az sql db tde create -d "dbtde" -g "Geeks_RG" -s "tdegenpacttest" --status Enabled 


  # get encryption scan progress
  az sql db tde list-activity -d "dbtde" -g "Geeks_RG" -s "tdegenpacttest" 
